{
  "name": "ng2-auto-complete",
  "version": "0.7.9",
  "description": "Angular2 Input Autocomplete",
  "license": "MIT",
  "main": "dist/ng2-auto-complete.umd.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "start": "http-server",
    "build": "npm-run-all --serial clean build:ts build:umd",
    "build:ts": "ngc",
    "build:umd": "webpack",
    "test": "npm-run-all test:start test:webtest test:stop",
    "test:start": "forever start --silent node_modules/http-server/bin/http-server",
    "test:stop": "forever stop --silent node_modules/http-server/bin/http-server",
    "test:webtest": "webtest ng2-auto-complete.webtest.txt"
  },
  "devDependencies": {
    "@angular/common": "^2.2.4",
    "@angular/compiler": "^2.2.4",
    "@angular/compiler-cli": "^2.2.4",
    "@angular/core": "^2.2.4",
    "@angular/forms": "^2.2.4",
    "@angular/http": "^2.2.4",
    "@angular/platform-browser": "^2.2.4",
    "@angular/platform-browser-dynamic": "^2.2.4",
    "@angular/router": "^3.2.4",
    "@angular/upgrade": "^2.2.4",
    "@angular2-material/core": "^2.0.0-alpha.8-2",
    "@angular2-material/input": "^2.0.0-alpha.8-2",
    "@ngtools/webpack": "^1.1.6",
    "@types/node": "^6.0.31",
    "angular2-template-loader": "^0.4.0",
    "core-js": "^2.4.1",
    "forever": "^0.15.2",
    "http-server": "^0.9.0",
    "ng2-inline-template": "^0.1.1",
    "npm-run-all": "^3.1.1",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.5.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "ts-loader": "~0.8.2",
    "typescript": "2.0.10",
    "web-test": "^0.2.6",
    "webpack": "2.1.0-beta.25",
    "zone.js": "^0.6.21"
  },
  "peerDependencies": {
    "@angular/common": "^2.2.4",
    "@angular/core": "^2.2.4",
    "@angular/forms": "^2.2.4",
    "@angular/http": "^2.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng2-ui/ng2-auto-complete.git"
